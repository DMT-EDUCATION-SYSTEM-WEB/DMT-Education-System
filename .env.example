# Contents of /dmt-edu-ui/dmt-edu-ui/.env.example

# Example environment variables for DMT Education System

# API base URL
VITE_API_BASE_URL=https://api.dmt-edu.com

# Authentication settings
VITE_AUTH_TOKEN_SECRET=your_secret_key_here
VITE_AUTH_TOKEN_EXPIRY=1h

# Database settings
VITE_DB_HOST=localhost
VITE_DB_PORT=5432
VITE_DB_USER=your_db_user
VITE_DB_PASSWORD=your_db_password
VITE_DB_NAME=dmt_edu

# Third-party service configurations
VITE_PAYMENT_GATEWAY_URL=https://payment.dmt-edu.com
VITE_GOOGLE_CALENDAR_API_KEY=your_google_calendar_api_key
VITE_SMS_PROVIDER_API_KEY=your_sms_provider_api_key

# Feature flags
VITE_ENABLE_2FA=true
VITE_ENABLE_LOGGING=true

# Other configurations
VITE_APP_NAME=DMT Education System
VITE_APP_VERSION=1.0.0

# Note: Replace the placeholder values with actual configuration values as needed.