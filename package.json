{
  "name": "dmt-edu-ui",
  "version": "1.0.0",
  "description": "A web application for DMT education center management.",
  "main": "src/main.tsx",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write .",
    "analyze": "vite build --mode analyze",
    "cleanup:unused": "eslint . --ext .ts,.tsx --fix",
    "bundle-size": "vite build && du -sh dist/",
    "check-unused": "npx depcheck",
    "replace-emojis": "node scripts/replace-emojis.js",
    "restore-emojis": "node scripts/restore-emojis.js",
    "cleanup-imports": "node scripts/cleanup-imports.js",
    "comprehensive-icons": "node scripts/comprehensive-icon-replacement.js",
    "fix-wrong-replacements": "node scripts/fix-wrong-replacements.js",
    "final-emoji-cleanup": "node scripts/final-emoji-cleanup.js",
    "analyze-icons": "node scripts/analyze-icons.js"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@reduxjs/toolkit": "^1.9.5",
    "aos": "^2.3.4",
    "axios": "^0.27.2",
    "framer-motion": "^5.6.0",
    "lucide-react": "^0.541.0",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-big-calendar": "^1.8.2",
    "react-countup": "^6.5.3",
    "react-dom": "^17.0.2",
    "react-helmet-async": "^2.0.5",
    "react-intersection-observer": "^10.0.0",
    "react-redux": "^8.1.1",
    "react-router-dom": "^6.14.1",
    "react-spring": "^9.7.5",
    "recharts": "^3.3.0",
    "redux": "^4.2.1"
  },
  "devDependencies": {
    "@types/aos": "^3.0.7",
    "@types/react": "^17.0.62",
    "@types/react-dom": "^17.0.20",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "@vitejs/plugin-react": "^1.3.2",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.44.0",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.6",
    "typescript": "^4.9.5",
    "vite": "^2.9.18"
  },
  "keywords": [
    "education",
    "management",
    "react",
    "redux",
    "vite"
  ],
  "author": "zero",
  "license": "MIT"
}
